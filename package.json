{
  "name": "open-heal-components",
  "version": "0.0.11",
  "description": "Repositório de componentes desenvolvido para a aplicação Open Heal.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "jsnext:main": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/open-heal-components/open-heal-components.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/",
    "react-components/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6006 -s dist",
    "build-storybook": "storybook build -s dist",
    "storybook-stencil-build": "rm -rf react-components/ dist/ loader/ && stencil build --docs && storybook dev -p 6006 -s dist"
  },
  "dependencies": {
    "@stencil/core": "^3.0.0",
    "@storybook/web-components": "^7.0.8",
    "stencil-tailwind-plugin": "^1.7.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "yarn-deduplicate": "^6.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@stencil/react-output-target": "^0.5.0",
    "@stencil/sass": "^3.0.1",
    "@storybook/addon-essentials": "7.0.8",
    "@storybook/addon-interactions": "7.0.8",
    "@storybook/addon-links": "7.0.8",
    "@storybook/blocks": "7.0.8",
    "@storybook/react": "7.0.8",
    "@storybook/react-webpack5": "7.0.8",
    "@storybook/testing-library": "0.0.14-next.2",
    "@types/babel__core": "^7.20.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "prop-types": "15.8.1",
    "puppeteer": "^19.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "7.0.8"
  },
  "license": "MIT"
}
